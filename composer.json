{
    "name": "noitran/opendox",
    "description": "OpenApi(Swagger) 3.0 package for Lumen 5.5+ and Laravel 5.5+ with REDOC UI and SwaggerUI 3",
    "authors": [
        {
            "name": "Noitran.Black",
            "email": "noitran.black@gmail.com"
        }
    ],
    "keywords": [
        "api",
        "openapi",
        "swagger",
        "swagger-ui",
        "laravel",
        "lumen",
        "rest",
        "documentation",
        "redoc",
        "redoc-ui"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3 | ^8.0",
        "ext-json": "*",
        "symfony/yaml": "^5.0 | ^6.0",
        "illuminate/routing": "^5|^6|^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7|^9.5.10",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "laravel/lumen-framework": "~8.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0"
                
    },
    "autoload": {
        "psr-4": {
            "Noitran\\Opendox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Noitran\\Opendox\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noitran\\Opendox\\ServiceProvider"
            ]
        }
    }
}
